{
  "name": "listening-lobby",
  "version": "1.0.0",
  "description": "A web application for listening to music with friends simultaneously via your music provider",
  "main": "index.js",
  "scripts": {
    "heroku-postbuild": "cd front-end && npm install && npm install --only=dev --no-shrinkwrap && npm run build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node server.js",
    "server": "nodemon server",
    "client": "npm start --prefix front-end",
    "dev": "concurrently \"npm run client\" \"npm run server\""
  },
  "engines": {
    "node": "14.17.0",
    "npm": "8.1.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alex-is-martin/listening-lobby.git"
  },
  "author": "Justin Fisher & Alexis Martin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/alex-is-martin/listening-lobby/issues"
  },
  "homepage": "https://github.com/alex-is-martin/listening-lobby#readme",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-regular-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.16",
    "axios": "^0.22.0",
    "config": "^3.3.6",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "fs": "0.0.1-security",
    "jsonwebtoken": "^8.5.1",
    "moments": "0.0.2",
    "react-scrollable-feed": "^1.3.1",
    "socket.io": "^4.3.1"
  },
  "devDependencies": {
    "concurrently": "^6.3.0",
    "nodemon": "^2.0.13"
  }
}
